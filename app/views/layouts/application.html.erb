<!DOCTYPE html>
<html>
<head>
  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>

  <%#= javascript_include_tag 'application' "data-turbolinks-track" => true %>
  <%#= javascript_include_tag 'jquery-2.2.4.min' "data-turbolinks-track" => true %>
  <%#= javascript_include_tag 'multislider' "data-turbolinks-track" => true %>
  <script type="text/javascript" src="http://code.jquery.com/jquery-1.9.1.js"></script>
  <script type="text/javascript" src="app/javascript/multislider.js"></script>
  <%= stylesheet_link_tag 'index', media: 'all', 'data-turbolinks-track': 'reload' %>


  <%#= javascript_include_tag 'application' "data-turbolinks-track" => true %>
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">


  <!-- VueJS -->
  <script src="https://unpkg.com/axios/dist/axios.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/vue/dist/vue.js"></script>
  <meta charset='utf-8'>
  <meta http-equiv='X-UA-Compatible' content='IE=edge'>
  <title><%= yield(:page_title).presence || 'Edfectively | Effective Future Education' %></title>
  <meta name='viewport' content='width=device-width, initial-scale=1'>

</head>
<body>

<div class="container">
  <%= render "shared/header" %>
  <div class="main">
    <%#= render 'shared/messages' %>
    <% if !@article_detail.present? %>
      <%= render partial: "shared/left_side", locals: {data: @data, course: @my_courses} %>
    <% end %>
    <%= yield %>
  </div>
  <div id="footer">
  </div>
</div>
</body>
</html>
