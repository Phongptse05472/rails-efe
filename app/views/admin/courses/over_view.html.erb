<div class="create-course-content">
  <%= render "add_course_progress" %>
  <div>
    <%= form_with model: Course.new do |f| %>
      <%= f.label :image, "Image:" %>
      <%= f.file_field :image %><br />
      <%= f.label :name, "Title:" %>
      <%= f.text_field :name %><br />
      <%= f.label :description, "Description:" %>
      <%= f.text_field :description %>
    <% end %>
  </div>
</div>
