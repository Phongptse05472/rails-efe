<head>
  <%= stylesheet_link_tag 'upload', media: 'all', 'data-turbolinks-track': 'reload' %>
</head>
<div class="process">
  <div class="process">
    <%= link_to previous_wizard_path, class: "previous" do %>
      <i class="fa fa-step-backward" aria-hidden="true"></i>
    <% end %>
    <% disable = session[:course_id].nil? ? "disable-click" : "" %>
    <%= link_to wizard_path(:landing_page), class: "landing_page" do %>
      <i class="fa fa-check-circle <%= 'active' if step.to_s == 'landing_page' %>"></i>
    <% end %>
    <%= link_to wizard_path(:create_content), class: "create_content #{disable}" do %>
      <i class="fa fa-check-circle <%= 'active' if step.to_s == 'create_content' %>" aria-hidden="true"></i>
    <% end %>
    <%= link_to wizard_path(:over_view), class: "over_view #{disable}" do %>
      <i class="fa fa-check-circle <%= 'active' if step.to_s == 'over_view' %>" aria-hidden="true"></i>
    <% end %>
    <%= link_to next_wizard_path, class: "next" do %>
      <i class="fa fa-step-forward" aria-hidden="true"></i>
    <% end %>
  </div>
</div>